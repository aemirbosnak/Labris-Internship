				{
					"listen": "test",
					"script": {
						"id": "5bb7dc1a-4cdc-40b0-8bae-ad1f397c802b",
						"type": "text/javascript",
						"exec": [
							"// Test to check the response body structure for Project object",
							"if (pm.request.method === 'GET' || pm.request.method === 'POST') {",
							"    pm.test(\"Response body structure is valid\", function () {",
							"        var responseJSON = pm.response.json();",
							"",
							"        // If the response is an array, check each project object in the array",
							"        if (Array.isArray(responseJSON)) {",
							"            responseJSON.forEach(function (project) {",
							"                pm.expect(project).to.be.an('object').that.has.all.keys(",
							"                    'id', 'parent_id', 'order', 'color', 'name',",
							"                    'comment_count', 'is_shared', 'is_favorite',",
							"                    'is_inbox_project', 'is_team_inbox', 'url', 'view_style'",
							"                );",
							"",
							"                // Type checks for each attribute",
							"                pm.expect(project.id).to.be.a('string');",
							"                pm.expect(project.parent_id).to.be.oneOf([null, 'string']);",
							"                pm.expect(project.order).to.be.a('number');",
							"                pm.expect(project.color).to.be.a('string');",
							"                pm.expect(project.name).to.be.a('string');",
							"                pm.expect(project.comment_count).to.be.a('number');",
							"                pm.expect(project.is_shared).to.be.a('boolean');",
							"                pm.expect(project.is_favorite).to.be.a('boolean');",
							"                pm.expect(project.is_inbox_project).to.be.a('boolean');",
							"                pm.expect(project.is_team_inbox).to.be.a('boolean');",
							"                pm.expect(project.url).to.be.a('string');",
							"                pm.expect(project.view_style).to.be.a('string');",
							"            });",
							"        }",
							"        // If the response is a single project object, check its structure and types directly",
							"        else if (typeof responseJSON === 'object') {",
							"            pm.expect(responseJSON).to.be.an('object').that.has.all.keys(",
							"                'id', 'parent_id', 'order', 'color', 'name',",
							"                'comment_count', 'is_shared', 'is_favorite',",
							"                'is_inbox_project', 'is_team_inbox', 'url', 'view_style'",
							"            );",
							"",
							"            // Type checks for each attribute",
							"            pm.expect(responseJSON.id).to.be.a('string');",
							"            pm.expect(responseJSON.parent_id).to.be.oneOf([null, 'string']);",
							"            pm.expect(responseJSON.order).to.be.a('number');",
							"            pm.expect(responseJSON.color).to.be.a('string');",
							"            pm.expect(responseJSON.name).to.be.a('string');",
							"            pm.expect(responseJSON.comment_count).to.be.a('number');",
							"            pm.expect(responseJSON.is_shared).to.be.a('boolean');",
							"            pm.expect(responseJSON.is_favorite).to.be.a('boolean');",
							"            pm.expect(responseJSON.is_inbox_project).to.be.a('boolean');",
							"            pm.expect(responseJSON.is_team_inbox).to.be.a('boolean');",
							"            pm.expect(responseJSON.url).to.be.a('string');",
							"            pm.expect(responseJSON.view_style).to.be.a('string');",
							"        }",
							"        // If the response is neither an object nor an array, the test will fail",
							"        else {",
							"            pm.expect.fail(\"Invalid response format. Expected an object or an array.\");",
							"        }",
							"    });",
							"}"
						]
					}
				}
